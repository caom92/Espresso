{
  "Standard Log Table": {
    "prefix": "fsm-log-table",
    "description": "Standard log table",
    "body": [
      "namespace fsm\\database\\\\${program}\\\\${module}\\\\${log};",
      "require_once realpath(dirname(__FILE__).'/../../../../dao/LogTable.php');",
      "use fsm\\database as db;",
      "",
      "class Logs extends db\\LogTable",
      "{",
      "\tfunction __construct() { ",
      "\t\tparent::__construct('${prefix}_logs');",
      "\t}",
      "",
      "\tfunction selectByCaptureDateID(\\$dateID) {",
      "\t\treturn parent::select(",
      "\t\t\t[",
      "",
      "\t\t\t],",
      "\t\t\t[",
      "\t\t\t\t'capture_date_id' => $dateID,",
      "\t\t\t]",
      "\t\t);",
      "\t}",
      "}"
    ]
  },
  "Standard Insertable Table": {
    "prefix": "fsm-insertable-table",
    "description": "Standard insertable table",
    "body": [
      "namespace fsm\\database\\\\${program}\\\\${module}\\\\${log};",
      "require_once realpath(dirname(__FILE__).'/../../../../dao/InsertableTable.php');",
      "use fsm\\database as db;",
      "",
      "class ${ClassName} extends db\\InsertableTable",
      "{",
      "\tfunction __construct() { ",
      "\t\tparent::__construct('${prefix}_${table}');",
      "\t}",
      "}"
    ]
  },
  "Standard Authorization Report": {
    "prefix": "fsm-authorization-report",
    "description": "Standard authorization report service",
    "body": [
      "require_once realpath(dirname(__FILE__).'/../../../service_creators.php');",
      "",
      "\\$service = fsm\\createAuthorizationReportService(",
      "\t'${program}',",
      "\t'${module}',",
      "\t'${log}',",
      "\t[",
      "\t\t'items_name' => 'items',",
      "\t\t'extra_info' => [",
      "\t\t\t// NULL",
      "\t\t],",
      "\t\t'function' => function(\\$scope, \\$segment, \\$logDate) {",
      "\t\t\t\\$items = \\$scope->daoFactory->get('${LogsTable}')",
      "\t\t\t\t->selectByCaptureDateID(\\$logDate['id']);",
      "",
      "\t\t\treturn \\$items;",
      "\t\t}",
      "\t]",
      ");"
    ]
  },
