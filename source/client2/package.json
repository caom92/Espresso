{
  "name": "frontend",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build-prod": "ng build --prod --aot=false",
    "lint": "ng lint",
    "less-compile": "lessc --clean-css='--s1 --advanced --compatibility=ie8' dev/stylesheets/less/main.less dev/stylesheets/app.style.css",
    "less-compile-win": "lessc --clean-css=\"--s1 --advanced --compatibility=ie8\" dev\\stylesheets\\less\\main.less dev\\stylesheets\\app.style.css",
    "dev-build": "lessc --clean-css='--s1 --advanced --compatibility=ie8' dev/stylesheets/less/main.less dev/stylesheets/app.style.css && pug dev/templates/pug -o dev/templates/",
    "dev-build-win": "lessc --clean-css=\"--s1 --advanced --compatibility=ie8\" dev\\stylesheets\\less\\main.less dev\\stylesheets\\app.style.css && pug dev\\templates\\pug -o dev\\templates\\",
    "pug-compile": "pug ./dev/components -o ./dev/components --doctype html"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": ">=4.3.3",
    "@angular/common": ">=4.0.0",
    "@angular/compiler": ">=4.0.0",
    "@angular/core": ">=4.0.0",
    "@angular/forms": ">=4.0.0",
    "@angular/http": ">=4.0.0",
    "@angular/platform-browser": ">=4.0.0",
    "@angular/platform-browser-dynamic": ">=4.0.0",
    "@angular/router": ">=4.0.0",
    "@mdi/font": ">=3.0.39",
    "@uirouter/angular": ">=1.0.1",
    "angular-l10n": ">=5.0.0",
    "angular2-pubsub": ">=4.0.0",
    "core-js": ">=2.4.1",
    "file-saver": ">=1.3.8",
    "jquery": ">=3.2.1",
    "materialize-css": ">=0.100.2",
    "mdi": ">=2.2.43",
    "ng2-dragula": ">=1.5.0",
    "ngx-materialize": ">=6.1.3",
    "ngx-papaparse": "^3.0.0-beta.0",
    "rxjs": "^6.2.2",
    "rxjs-compat": "^6.2.2",
    "zone.js": ">=0.8.14"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.13.7",
    "@angular/cli": ">=1.2.4",
    "@angular/compiler-cli": ">=4.0.0",
    "@angular/language-service": ">=4.0.0",
    "@types/node": ">=6.0.60",
    "codelyzer": ">=3.0.1",
    "less": ">=2.7.2",
    "less-plugin-clean-css": ">=1.5.1",
    "pug-cli": ">=1.0.0-alpha6",
    "ts-node": ">=3.0.4",
    "tslint": ">=5.3.2",
    "typescript": "~2.7.2"
  }
}
