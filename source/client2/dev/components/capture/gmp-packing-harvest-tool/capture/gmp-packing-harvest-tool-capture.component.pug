extends ../../capture.common.pug

block reportViewer
  gmp-packing-harvest-tool-report-viewer

block logViewer
  gmp-packing-harvest-tool-log-list([suffix]='suffix', *ngIf='newLog !== true', (startLog)='onOpenLog($event)')
  gmp-packing-harvest-tool-log(*ngIf='newLog == true', [log]='logData', (closeLog)='onCloseLog($event)')